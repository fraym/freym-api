{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "release": {
            "dependsOn": [
                "^release"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {},
        "fix": {},
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}
